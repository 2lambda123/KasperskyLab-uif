    "postpublish": "npm run publish:showReadme && yarn gulp clear-lib-folder",
    "check-dependencies": "node check-dependencies.js",
    "publish:hideReadme": "mv 'README.md' 'NOT-PUBLISHED.md'",
    "publish:showReadme": "mv 'NOT-PUBLISHED.md' 'README.md'",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "release": "yarn set-env gulp release",
    "set-env": "./node_modules/@kl/dev-tools/node_modules/.bin/env-cmd -f ./node_modules/@kl/dev-tools/src/utils/get-node-options.js"
  },
