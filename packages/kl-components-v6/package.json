  "version": "0.39.0",
  "description": "Components for building UI's",
  "main": "src/index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "build": "npm i && gulp clear && gulp build && tsc --project tsconfig.build.json --outDir ./esm && tsc-alias -v --dir ./esm --project tsconfig.build.json",
    "release": "npm run build && gulp deploy",
    "build-publish": "npm run build && cd ./esm && npm publish ./ --no-git-tag-version",
    "update-colors": "gulp updateColors",
    "lint": "npm run lint-es && npm run lint-ts",
    "lint-es": "eslint ./src",
    "lint-es:fix": "eslint --fix ./src",
    "lint-ts": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "storybook:ci": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "jest:screenshots": "jest --runInBand --useStderr --verbose -i -c screenshots/jest.config.js",
    "jest:screenshots-update": "jest --runInBand --useStderr --verbose -i -c screenshots/jest.config.js -u",
    "test:screenshots": "start-server-and-test storybook:ci http-get://localhost:6006 jest:screenshots",
    "test:screenshots-update": "start-server-and-test storybook:ci http-get://localhost:6006 jest:screenshots-update"
  },
  "sideEffects": [
    "*.less",
    "design-system/assets/*",
    "design-system/global-style/*",
    "helpers/localization/i18next.js"
  ],
  "overrides": {
    "es5-ext": "0.10.46",
    "@typescript-eslint/*": "5.11.0"
